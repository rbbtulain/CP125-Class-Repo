    if len(traceroute) < 2:
        return -1
    largest_jump = 0
    bottleneck_index = -1
    for i in range(1, len(traceroute)):
        previous_latency = traceroute[i - 1][1]
        current_latency = traceroute[i][1]
        jump = current_latency - previous_latency
        if jump > largest_jump:
            largest_jump = jump
            bottleneck_index = i - 1
    return bottleneck_index